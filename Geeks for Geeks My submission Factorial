
import java.util.*;
import java.lang.*;
import java.io.*;

class GFG {
    public static long factorial(long a){
    if(a==0||a==1)
    return 1;
    else
    return a*factorial(a-1);
      
    }
	public static void main (String[] args) {
    Scanner sc=new Scanner(System.in);
    int tc=sc.nextInt();
  
    while(tc!=0){
         long number=sc.nextLong();
         System.out.println(factorial(number));
        tc--;
    }
	}
}
